datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  username String   @unique
  password String
  groups   Group[]  @relation("GroupMembers") // Relation avec les groupes
}


model Group {
  id       Int      @id @default(autoincrement())
  name     String
  // Autres détails du groupe à ajouter

  // Relation avec l'utilisateur
  members  User[]   @relation("GroupMembers")
}



generator client {
  provider = "prisma-client-js"
}
